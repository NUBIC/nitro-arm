%span{ class: 'domain_option', id: "#{provider}", title: t("login.#{provider}")}
  .domain_option_image
    = link_to image_tag("#{provider}_64.png"), send("user_#{provider}_omniauth_authorize_path"), id: "#{provider}_link"
  .domain_name
    = link_to(h(provider.upcase), send("user_#{provider}_omniauth_authorize_path"))